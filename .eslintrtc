{
  "extends": "airbnb",
  "globals": {
    "_": false,
    "R7Promise": false,
    "renderTemplate": false,
  },
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 1, // Commas at the end of objects, deactived in airbnb config
    "no-cond-assign": 0,
    "func-names": 0, // #TRANSVERSE-357
    "key-spacing": 0, // #TRANSVERSE-358
    "new-cap": 0, // #TRANSVERSE-359
    "no-var": 0, // #TRANSVERSE-360
    "one-var": 0, // #TRANSVERSE-361
    "vars-on-top": 0, // #TRANSVERSE-362
    "object-shorthand": 0, // #TRANSVERSE-238
    "quote-props": 0, // #TRANSVERSE-363
    "no-param-reassign": 0, // #TRANSVERSE-364
    "max-len": 0, // #TRANSVERSE-365
    "prefer-arrow-callback": 0, // #TRANSVERSE-366
    "arrow-body-style": 0, // #TRANSVERSE-370
    "prefer-template": 0, // #TRANSVERSE-367
    "no-unneeded-ternary": 0, // #TRANSVERSE-368
    "wrap-iife": 0 // #TRANSVERSE-369
  }
}
